{

  "manifest_version": 2,
  "name": "Moodle Session Extender",
  "version": "1.0",
  "author": "Dominique Narbel",

  "description": "Keeps you logged in for better user experience :)",

  "icons": {
    "48": "icons/moodle.png"
  },

  "permissions": [
    "activeTab",
    "storage",
    "cookies",
    "*://*/*"
  ],

  "browser_action": {
    "default_icon": "icons/moodle.png",
    "default_title": "Moodle",
    "default_popup": "popup/options.html"
  },

  "content_scripts": [
    {
      "matches": ["https://lms.fh-wedel.de/*"],
      "js": []
    }
  ]

}